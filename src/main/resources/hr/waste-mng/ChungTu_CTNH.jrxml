	<queryString language="SQL">
		<![CDATA[select 
	D.waste_type, T.name as waste_type_name, T.code, T.unit, T.waste_status, T.handle_method, T.code,
    (select H.name from waste_handle_method H where H.id = T.handle_method) as handle_method_name,
    (select S.name from waste_status S where S.id = T.waste_status) as waste_status_name,
    D.price, round(sum(D.weight),2) as weight, group_concat(D.id) as ids 
from waste_detail_data D 
inner join waste_type T on D.waste_type = T.id 
where 1 = 1 
and D.waste_master = 8 
group by D.waste_type order by D.waste_type asc]]>
	</queryString>
